/* Navigation Division
------------------------------------*/
.navbar ul{
	font-family: 'EB Garamond', serif;
	list-style: none;
	background: #546B60;
	position: float;
	padding: 5px;
	text-align: center;
	font-size: 20px;
	font-weight: bold;
	text-shadow: 1px -1px 0 #343434;	
}
.navbar li{
	display: inline-block;
	
}
.navbar a{
	text-decoration: none;
	color: #F7FFF7;
	width: 120px;
	display: block;
	padding: 20px;	
	text-align: center;
	
}
.navbar a:hover{
	background: #468499;
}
/* Body Division
------------------------------------*/
body {
	background: #546B60;
	background: -moz-linear-gradient(top, #546B60 0%, #F7FFF7 72%, #343434 100%);
	background: -webkit-linear-gradient(top, #546B60 0%, #F7FFF7 72%, #343434 100%);
	background: linear-gradient(to bottom, #546B60 0%, #F7FFF7 72%, #343434 100%);
	background-repeat: no-repeat;
	background-size: cover;
}

h3 {
	font-family: 'EB Garamond', serif;
    font-size: 50px;
    font-style: Bold;
	color: #468499;
	display: block;
	text-shadow: 1px -1px 0 #F7FFF7;
	text-align: center;
}	
* {box-sizing:border-box}

/* Slideshow container */
.slideshow-container {
  max-width: 1000px;
  position: relative;
  margin: auto;
}

/* Hide the images by default */
.mySlides {
  display: none;
}

/* Next & previous buttons */
.prev, .next {
  cursor: pointer;
  position: absolute;
  top: 50%;
  width: auto;
  margin-top: -25px;
  padding: 16px;
  color: white;
  font-weight: bold;
  font-size: 18px;
  transition: 0.6s ease;
  border-radius: 0 3px 3px 0;
  user-select: none;
}

/* Position the "next button" to the right */
.next {
  right: 0;
  border-radius: 3px 0 0 3px;
}

/* On hover, add a black background color with a little bit see-through */
.prev:hover, .next:hover {
  background-color: rgba(0,0,0,0.8);
}

/* Caption text */
.text {
  color: #f2f2f2;
  font-family: 'EB Garamond', serif;
  font-size: 40px;
  padding: 8px 12px;
  position: absolute;
  bottom: 8px;
  width: 100%;
  text-align: center;
  text-shadow: 4px 3px 0 #343434;
}

/* Number text (1/3 etc) */
.numbertext {
  color: #f2f2f2;
  font-size: 12px;
  padding: 8px 12px;
  position: absolute;
  top: 0;
}

/* The dots/bullets/indicators */
.dot {
  cursor: pointer;
  height: 15px;
  width: 15px;
  margin: 0 2px;
  background-color: #bbb;
  border-radius: 50%;
  display: inline-block;
  transition: background-color 0.6s ease;
}

.active, .dot:hover {
  background-color: #717171;
}

/* Fading animation */
.fade {
  -webkit-animation-name: fade;
  -webkit-animation-duration: 1.5s;
  animation-name: fade;
  animation-duration: 1.5s;
}

@-webkit-keyframes fade {
  from {opacity: .4}
  to {opacity: 1}
}

@keyframes fade {
  from {opacity: .4}
  to {opacity: 1}
}


/* Footer Division
------------------------------------*/
*  {
	padding: 0;
	margin: 0;
	box-sizing: border-box;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 20px;
}
.footer-container {
	background-color: #546B60;
	padding-bottom: 4rem;
}
.footer {
	width: 75%;
	height: 25vh;
	background-color: #546B60;
	color: #F7FFF7;
	display: flex;
	justify-content: center;
	flex-wrap: wrap;
	margin: 0 auto;
	padding-bottom: 9rem;
	padding-left: 0px;
	padding-top: 100px;
	margin-right: 0px;
}
.footer-heading {
	display: flex;
	flex-direction: column;
	margin-right: 4rem;	
}

.footer-heading h2 {
	margin-bottom: 2rem;
}
.footer-heading a {
	color: #F7FFF7;
	text-decoration: none;
	margin-bottom: 0.5rem;
}

.footer-heading a:hover{
	color: #468499;
}

.footer-heading footer-1 {
	color: #F7FFF7;
	text-decoration: none;
	margin-bottom: 0.5rem;	
}.

.footer-heading footer-1 a:hover{
	color: #468499;
}
li {
	list-style: none;
}
h6 {
	color: #343434;
}





