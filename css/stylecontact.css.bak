/* Navigation Division
------------------------------------*/
.navbar ul{
	font-family: 'EB Garamond', serif;
	list-style: none;
	background: #546B60;
	position: float;
	padding: 5px;
	text-align: center;
	font-size: 20px;
	font-weight: bold;
	text-shadow: 1px -1px 0 #343434;	
}
.navbar li{
	display: inline-block;
	
}
.navbar a{
	text-decoration: none;
	color: #F7FFF7;
	width: 120px;
	display: block;
	padding: 20px;	
	text-align: center;
	
}
.navbar a:hover{
	background: #468499;
}
/* Heading Division
------------------------------------*/
body {
	background-image: url(../img/background1.jpeg);
	background-repeat: no-repeat;
	background-size: cover;
	text-align: center;
}

h3 {
	font-family: 'EB Garamond', serif;
	color: #343434;
	text-align: Center;
	text-shadow: 2px -2px 0 #F7FFF7;
	font-size: 100px;
}
label {
	font-family: 'EB Garamond', serif;
	color: #345511;
	font-size: 25px;
	font-style: bold;
	display: block;
    font-size: 25px;
}
message-box {
	font-family: 'EB Garamond', serif;
	color: #343434;
	font-size: 25px;
}
h4 {
	font-family: 'EB Garamond', serif;
	display: block;
    font-size: 25px;
    font-style: bold;
	color: #345511;
	padding-bottom: 15px;
}
button {
	background-color: #345511;
	border: none;
	color: #F7FFF7;
	padding: 15px 32px;
	text-align: center;
	text-decoration: none;
	display: inline-block;
	font-size: 16px;
}
button:hover {
	background-color: #468499;
	color: #F7FFF7;
	transition: 0.3 ease-out;
}
	
/* Footer Division
------------------------------------*/
*  {
	padding: 0;
	margin: 0;
	box-sizing: border-box;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 20px;
}
.footer-container {
	background-color: #546B60;
	padding-bottom: 4rem;
}
.footer {
	width: 75%;
	height: 25vh;
	background-color: #546B60;
	color: #F7FFF7;
	display: flex;
	justify-content: center;
	flex-wrap: wrap;
	margin: 0 auto;
	padding-bottom: 9rem;
	padding-left: 0px;
	padding-top: 100px;
	margin-right: 0px;
}
.footer-heading {
	display: flex;
	flex-direction: column;
	margin-right: 4rem;	
}
.footer-heading h2 {
	margin-bottom: 2rem;
}
.footer-heading a {
	color: #F7FFF7;
	text-decoration: none;
	margin-bottom: 0.5rem;
}
.footer-heading a:hover{
	color: #468499;
	transition: 0.3 ease-out;
}
li {
	list-style: none;
}
h6 {
	color: #343434;
}

	